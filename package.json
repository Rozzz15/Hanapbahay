  "scripts": {
    "start": "expo start",
    "start:simple": "expo start --clear",
    "start:lan": "expo start --clear --lan",
    "start:tunnel": "expo start --clear --tunnel",
    "start:localhost": "expo start --clear --localhost",
    "android": "expo run android",
    "ios": "expo run ios",
    "web": "expo start --web",
    "web:clean": "expo start --web --clear --port 8081",
    "web:fixed": "set EXPO_METRO_CONFIG=metro.config.web-fixed.js && expo start --web --clear --port 8081",
    "web:only": "set EXPO_METRO_CONFIG=metro.config.web-only.js && expo start --web --clear --port 8081",
    "web:basic": "expo start --web --clear --port 8081 --entry-point app/web-basic.tsx",
    "web:working": "expo start --web --clear --port 8081",
    "web:simple": "set EXPO_METRO_CONFIG=metro.config.web-simple.js && expo start --web --clear --port 8081",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "reset": "expo start --clear --reset-cache"
  },
